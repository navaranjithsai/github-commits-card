  "author": "navaranjithsai",
  "license": "MIT",
  "devDependencies": {
    "@cloudflare/kv-asset-handler": "^0.4.1",
    "@cloudflare/workers-types": "^4.20240117.0",
    "@types/node": "^20.10.0",
    "@vercel/node": "^3.0.0",
    "@vitest/coverage-v8": "^1.1.0",
    "typescript": "^5.3.0",
    "vercel": "^33.0.0",
    "vitest": "^1.1.0",
    "wrangler": "^3.22.0"
  }
